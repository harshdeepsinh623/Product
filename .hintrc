{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off",
        "empty-table-header": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "float: inline-end",
          "backdrop-filter",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ]
  }
}